    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "envFile": "${workspaceFolder}/ps5-mqtt/.env",
            "program": "${workspaceFolder}/ps5-mqtt/dev.js",
            "outFiles": [
                "${workspaceFolder}/ps5-mqtt/server/dist/**/*.js",
                "${workspaceFolder}/ps5-mqtt/dev.js"
            ]
        }
    ]
}